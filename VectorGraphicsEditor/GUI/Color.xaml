<UserControl x:Class="VectorGraphicsEditor.GUI.Color"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VectorGraphicsEditor.GUI"
             mc:Ignorable="d" 
                
             d:DesignHeight="32" d:DesignWidth="32">
    <Grid>
        <Rectangle Fill="Transparent"></Rectangle>
        <Rectangle Stroke="Black"></Rectangle>
        <Rectangle Name="Rectangle" Fill="Black" Margin="4px"></Rectangle> 
    </Grid>
</UserControl>
